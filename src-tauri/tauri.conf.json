{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "geni",
  "version": "0.1.10",
  "identifier": "com.dadangsutanto.geni",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "geni",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.13",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": "entitlements/macos.entitlements"
    }
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEJFQTk2Q0I4OThFRkQyMEQKUldRTjB1K1l1R3lwdm43czU2NEJSTENiQXMwT3Nya1cvQ1huQ016cFB4RE0xVmZ3S3FlOFBJeVUK",
      "endpoints": [
        "https://github.com/dadangsutanto/geni/releases/latest/download/latest.json"
      ]
    }
  }
}
